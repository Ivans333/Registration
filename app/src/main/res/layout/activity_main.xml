<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:text="Username:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView9"
            app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.47"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.078"/>
    <TextView
            android:text="Password:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            app:layout_constraintEnd_toStartOf="@+id/editTextTextPassword"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView9"
            android:layout_marginTop="54dp" app:layout_constraintHorizontal_bias="0.485"/>
    <Button
            android:text="submit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button"
            app:backgroundTint="#010101"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.498"
            android:onClick="submit"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonCity"
            app:layout_constraintVertical_bias="0.192"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTextTextPersonName2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.062"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2"
            android:layout_marginTop="28dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/editTextTextEmailAddress"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword"
            android:layout_marginTop="28dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/editTextPhone2"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress"
            android:layout_marginTop="28dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTextTextPersonCity"
            app:layout_constraintTop_toBottomOf="@+id/editTextPhone2"
            android:layout_marginTop="20dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.502"/>
    <TextView
            android:text="Email:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView7"
            app:layout_constraintEnd_toStartOf="@+id/editTextTextEmailAddress"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintTop_toBottomOf="@+id/textView8" android:layout_marginTop="54dp"/>
    <TextView
            android:text="Phone Number:"
            android:layout_width="64dp"
            android:layout_height="42dp" android:id="@+id/textView10"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            android:layout_marginTop="40dp" app:layout_constraintEnd_toStartOf="@+id/editTextPhone2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.513"/>
    <TextView
            android:text="City:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView11"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonCity"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="36dp"
            app:layout_constraintHorizontal_bias="0.567"/>
    <TextView
            android:text=""
            android:layout_width="203dp"
            android:layout_height="22dp" android:id="@+id/sPassword"
            app:layout_constraintTop_toBottomOf="@+id/sUsername" app:layout_constraintStart_toStartOf="@+id/sUsername"
            app:layout_constraintEnd_toEndOf="@+id/sUsername" android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text=""
            android:layout_width="203dp"
            android:layout_height="22dp" android:id="@+id/sCity"
            app:layout_constraintStart_toStartOf="@+id/sPhone" app:layout_constraintEnd_toEndOf="@+id/sPhone"
            app:layout_constraintTop_toBottomOf="@+id/sPhone" android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text=""
            android:layout_width="203dp"
            android:layout_height="22dp" android:id="@+id/sEmail"
            app:layout_constraintTop_toBottomOf="@+id/sPassword" app:layout_constraintEnd_toEndOf="@+id/sPassword"
            app:layout_constraintStart_toStartOf="@+id/sPassword" android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text=""
            android:layout_width="203dp"
            android:layout_height="22dp" android:id="@+id/sPhone"
            app:layout_constraintTop_toBottomOf="@+id/sEmail" app:layout_constraintEnd_toEndOf="@+id/sEmail"
            app:layout_constraintStart_toStartOf="@+id/sEmail" android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text=""
            android:layout_width="203dp"
            android:layout_height="22dp" android:id="@+id/sUsername"
            app:layout_constraintTop_toBottomOf="@+id/button"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.056"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.129"/>
</androidx.constraintlayout.widget.ConstraintLayout>